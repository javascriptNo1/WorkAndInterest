.success{
  font-size: 30rpx;
  padding: 20rpx;
}

.tips{
  margin: 100rpx auto;
  color: #e6e6e6;
  text-align: center;
}

.container{
  font-size: 30rpx;
  margin-top: 20rpx;
}

.paper{
  width: 90%
}

.paper .title{
  text-align: left;
  padding-left: 0rpx;
}

.clean:after{
  display: block;
  width: 0;
  height: 0;
  clear: both;
  content:"";
}

.rg{
  float: right
}
.lf{
  float: left
}



.itmes{
  width: 90%;
  background: #f5f5f5c2;
  margin-bottom: 30rpx;
  padding-top: 20rpx;
  border-radius: 30rpx; 
}

.title{
  font-size: 36rpx;
  padding-left: 20rpx;
  display: block;
  margin-bottom: 30rpx;
}

.options{
  font-size: 30rpx;
  margin-bottom: 10rpx;
  margin: 0 auto 10rpx auto;
  padding: 20rpx 20rpx;
  width: 90%;
  border-radius: 5rpx;
}

.red{
  border: 1rpx solid red;
  color: red;
  border-radius: 30rpx;
}
.green{
  border: 1rpx solid green;
  color: green;
  border-radius: 30rpx;
}

.opencomment{
  padding: 15rpx 10rpx 16rpx 30rpx;
  font-size: 26rpx;
}

.paperpoencomment{
   padding: 20rpx 10rpx 16rpx 30rpx;
}

.opencomment image{
  width: 46rpx;
  height: 46rpx;
  margin-right: 30rpx;
}
.opencomment .cloo{
  width:54rpx;
  height:54rpx;
}
.opencomment button{
  padding: 0rpx 18rpx;
  text-align: center;
  font-size: 28rpx;
  margin-right: 20rpx;
  float: right;
}

.but{
   background-color: #fb7299;
   color: #fff;
   margin-bottom: 30rpx;
   background: -webkit-gradient(linear, left top, left bottom, from(#fb7299), to(#E84B6E) );
}

.but2{
  margin-top: 200rpx;
  width: 40%;
  margin: 200rpx auto 30rpx auto;
}

.share{
    width:32%;
    background-color: #fb7299;
    color: #fff;
    font-size: 30rpx;
    background: -webkit-gradient(linear, left top, left bottom, from(#fb7299), to(#E84B6E) );
}